[
    {
        "title": "显示对话框——$.dialog(option)",
        "attr": [
            {
                "name": "title",
                "meaning": "对话框标题",
                "type": "字符",
                "default": "'对话框'"
            },
            {
                "name": "content",
                "meaning": "对话框内容",
                "type": "字符",
                "default": "null"
            },
            {
                "name": "hideX",
                "meaning": "是否隐藏右上角关闭按钮",
                "type": "布尔",
                "default": "false"
            },
            {
                "name": "buttons",
                "meaning": "对话框按钮数组",
                "type": "对象数组",
                "default": "null",
                "sub": {
                    "attr": [
                        {
                            "name": "text",
                            "meaning": "按钮文本",
                            "type": "字符",
                            "default": "null"
                        },
                        {
                            "name": "callback",
                            "meaning": "按钮点击回调函数",
                            "type": "函数",
                            "default": "null（点击后关闭对话框）"
                        }
                    ]
                }
            },
            {
                "name": "timeout",
                "meaning": "自动关闭延时（毫秒）",
                "type": "数字",
                "default": "null"
            },
            {
                "name": "size",
                "meaning": "对话框大小",
                "type": "对象|'full'",
                "default": "null",
                "sub": {
                    "attr": [
                        {
                            "name": "width",
                            "meaning": "宽度",
                            "type": "数字",
                            "default": "null（自适应）"
                        },
                        {
                            "name": "height",
                            "meaning": "高度",
                            "type": "数字",
                            "default": "null（自适应）"
                        }
                    ]
                }
            },
            {
                "name": "drag",
                "meaning": "是否允许拖动对话框",
                "type": "布尔",
                "default": "true"
            },
            {
                "name": "lock",
                "meaning": "是否添加背景锁定",
                "type": "布尔",
                "default": "true"
            },
            {
                "name": "showBack",
                "meaning": "对话框显示回调函数",
                "type": "函数",
                "default": "null"
            },
            {
                "name": "closeBack",
                "meaning": "对话框关闭回调函数",
                "type": "函数",
                "default": "null"
            },
            {
                "name": "padding",
                "meaning": "对话框内容padding",
                "type": "字符（写法同CSS）|'iframe'",
                "default": "null"
            }
        ]
    },
